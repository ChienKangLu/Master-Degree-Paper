    body()
        div(id="map",class="map-v3")
        div(class="header-content")
            div(class="header")
                div Trip Planning
                    //- span(class="header-small-txt") online
            div(class="user-header")
                if sess.login
                    div(class="user-header-item" id="userName") #{sess.name} 您好!
                else
                    div(class="user-header-item" id="userName") 遊客 您好!
                if sess.login
                    div(class="user-header-item" id="userPath") 
                        div(class="userPath-title") 旅遊紀錄
                        div(class="userPath-area hidden")
                            //- div(class="path") 路線1
                            //- div(class="path") 路線2
                            //- div(class="path") 路線3
                            //- div(class="path") 路線4
                if sess.login
                    div(class="user-header-item" id="logoutBtn" class="loginOutBtn") 登出
                else
                    div(class="user-header-item" id="loginBtn" class="loginOutBtn") 登入

        div(id="mapTool-area")
            img(id="mapTool",src="public/img/mapTool.svg")
            img(id="mapArea",class="hidden",src="public/img/mapArea.svg")
            img(id="mapClear",class="hidden",src="public/img/mapClear.svg")
        div(id="alert-area" class="alert-area hidden") 
            div(class="alert-box")
                div(class="alert-icon")
                    img(id="alert_icon_remind",class="alert_icon_remind",src="public/img/alert_icon_remind.svg")
                div(class="alert-title")
                div(class="alert-describe")
                div(class="understand remind-color" ) 知道了
        div(id="login-area" class="login-area hidden") 
            div(class="login-box")
                div(class="login-icon")
                    img(id="login_icon_remind",class="login_icon_remind",src="public/img/alert_icon_remind.svg")
                input(id="account",type="text",class="beauty-input",placeholder="Account")
                input(id="password",type="text",class="beauty-input",placeholder="Password")
                div(id="login",class="understand remind-color" ) 登入

        div(class="main-flex-container")
            div(class="tool-area")
                div(class="icon-tooltip")
                    img(id="datetime",class="datetime self-icon icon-active",src="public/img/datetime.svg")
                    div(class="icon-tooltip-text") 時間地點
                div(class="icon-tooltip")
                    img(id="poi",class="poi self-icon",src="public/img/poi.svg")
                    div(class="icon-tooltip-text") 景點數量
                div(class="icon-tooltip")
                    img(id="gear",class="gear self-icon",src="public/img/gear.svg")
                    div(class="icon-tooltip-text") 滿意參考
                div(class="icon-tooltip")
                    img(id="note",class="note self-icon",src="public/img/note.svg")
                    div(class="icon-tooltip-text") 路線規劃
                div(class="icon-tooltip")
                    img(id="analysis",class="analysis self-icon",src="public/img/analysis.svg")
                    div(class="icon-tooltip-text") 搜尋分析

            div(class="container op-area basic-area")
                div(class="row date")
                    div(class="col-xs-12 font-input-group")
                        div(class="myFont") Start
                        input(type="datetime-local",class="form-control",id="start_time",value="2017-11-30T08:00:00")
                div(class="row date")
                    div(class="col-xs-12 font-input-group")
                        div(class="myFont") End
                        input(type="datetime-local",class="form-control",id="end_time",value="2017-11-30T20:00:00")
                div(class="row where mobile-poi")
                    div(class="col-xs-12 font-input-group")
                        div(class="myFont") From
                        select(id="start_id_mobile" class="form-control")
                            each value, key in pois
                                if(value._id=="59e5a921d63dcb19ac9859fc")
                                    option(id=value._id,selected="selected")=value.name
                                else
                                    option(id=value._id) #{key+1} #{value.name}
                div(class="row where pc-poi")
                    div(class="col-xs-12 font-input-group")
                        div(class="myFont") From
                        input(type="text" name="team" id="start_id_pc" list="start_list" class="form-control" value="宜蘭車站") 
                        datalist(id="start_list")
                            each value, key in pois
                                option(id=value._id) #{value.name} 
                div(class="row where mobile-poi")
                    div(class="col-xs-12 font-input-group")
                        div(class="myFont") To
                        select(id="destination_id_mobile" class="form-control")
                            each value, key in pois
                                if(value._id=="59e5a84dd63dcb19ac984571")
                                    option(id=value._id,selected="selected")=value.name
                                else
                                    option(id=value._id) #{key+1} #{value.name}     
                div(class="row where pc-poi")
                    div(class="col-xs-12 font-input-group")
                        div(class="myFont") To
                        input(type="text" name="team" id="destination_id_pc" list="destination_list" class="form-control" value="養蜂人家-蜂采館") 
                        datalist(id="destination_list")
                            each value, key in pois
                                option(id=value._id) #{value.name} 
                div(class="row text-right")
                    a(id="search-v3",class="flat-btn btn-b",type="submit") search      
